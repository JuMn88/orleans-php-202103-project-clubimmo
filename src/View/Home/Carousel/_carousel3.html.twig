<div id="carouselExampleControls3" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    {% block carouselImages3 %}
      {% for photo in photos3 %}
        <div class="carousel-item {{ loop.first ? 'active' : '' }}">
          <img src="{{ photo.url }}" class="d-block w-100" alt="First slide">
        </div>
      {% else %}
        <img src="/assets/images/no_image.png" class="d-block w-100" alt="No image">
      {% endfor %}
    {% endblock %}
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls3" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">PRECEDENT</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls3" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">SUIVANT</span>
  </button>
</div>