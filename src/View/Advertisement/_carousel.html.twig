<div id="carouselExampleIndicators" class="carousel slide col-12 col-sm-9 p-0" data-ride="carousel">
  <div class="carousel-inner">
    {% block carouselImages %}
      {# Loop for display photos #}
      {% for photoAdress in photosAdress %}
        {# 1st photo must be active so index=1 #}
        {% if loop.index == (1) %}
          <div class="carousel-item active">
            <img class="d-block w-100" src="{{ photoAdress }}" alt="First slide">
          </div>
        {% else %}
          <div class="carousel-item">
            <img class="d-block w-100" src="{{ photoAdress }}" alt="Second slide">
          </div>
        {% endif %}
      {% endfor %}
    {% endblock %}
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">PRECEDENT</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">SUIVANT</span>
    </a>
  </div>
</div>
